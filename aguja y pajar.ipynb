    "#este codigo debe encontrar una aguja en un pajar\n",
    "import numpy as np\n",
    "#creacion de array\n",
    "pajar = np.array([1,5,3,12,15,\"aguja\"])\n",
    "#busqueda de aguja en el pajar\n",
    "for i, value in enumerate(pajar):\n",
    "    if value == \"aguja\":\n",
    "        print(\"encontre la aguja en la posicion \" + str(i))\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
